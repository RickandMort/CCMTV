<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 填充的颜色 -->
    <solid android:color="#FF6700" />

    <!-- 定义边框的颜色 -->
    <stroke
        android:width="1dp"
        android:color="#FF6700"
        android:dashGap="0dp"
        android:dashWidth="2dp" />

    <!-- 设置矩形的四个角为弧形 -->
    <corners android:radius="100dp" />

</shape>

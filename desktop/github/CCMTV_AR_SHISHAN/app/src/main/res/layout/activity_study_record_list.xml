<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context="com.linlic.ccmtv.yx.activity.user_statistics.StudyRecordListActivity">

    <include layout="@layout/activity_title8"/>

    <com.linlic.ccmtv.yx.widget.NodataEmptyLayout
        android:id="@+id/lt_nodata1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fff"
        android:visibility="gone"
        app:empty_topLine="true"/>

    <LinearLayout
        android:id="@+id/id_ll_study_record_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/id_lv_study_record_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:listSelector="@color/transparent2"/>

    </LinearLayout>

</LinearLayout>

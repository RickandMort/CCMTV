<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.linlic.ccmtv.yx.activity.my.our_video.MyOurResourcesDisplayActivity">

    <include layout="@layout/activity_title8"/>

    <FrameLayout
        android:id="@+id/id_webview_our_resources_display"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="match_parent"/>

    <com.github.barteksc.pdfviewer.PDFView
        android:id="@+id/id_pdf_view_our_resources_display"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>
